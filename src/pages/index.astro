    // Typing animation for hero
    const text = "Multiplying Success, One Division at a Time.";
    const typingElement = document.getElementById('typingText');
    let charIndex = 0;

    function typeText() {
      if (charIndex < text.length) {
        typingElement.textContent += text[charIndex];
        charIndex++;
        setTimeout(typeText, 80);
      }
    }

    setTimeout(typeText, 500);

    // Christmas snowflakes
    function createSnowflake() {
      const snowflake = document.createElement('div');
      snowflake.classList.add('snowflake');
      snowflake.innerHTML = ['‚ùÑ', '‚ùÖ', '‚ùÜ'][Math.floor(Math.random() * 3)];
      snowflake.style.left = Math.random() * 100 + 'vw';
      snowflake.style.animationDuration = (Math.random() * 3 + 7) + 's';
      snowflake.style.opacity = `${Math.random() * 0.5 + 0.2}`;
      snowflake.style.fontSize = (Math.random() * 0.8 + 0.5) + 'em';
      document.body.appendChild(snowflake);

      setTimeout(() => snowflake.remove(), 12000);
    }

    setInterval(createSnowflake, 400);

    // Smooth scroll to sections
    function scrollToSection(id) {
      const section = document.getElementById(id);
      section.scrollIntoView({ behavior: 'smooth', block: 'start' });
    }

    // Strong scroll snap - activate section when in view
    const allSections = document.querySelectorAll('section');
    
    const sectionObserver = new IntersectionObserver((entries) => {
      entries.forEach(entry => {
        if (entry.isIntersecting) {
          entry.target.classList.add('in-view');
        } else {
          // Only remove if not hero
          if (entry.target.id !== 'hero') {
            entry.target.classList.remove('in-view');
          }
        }
      });
    }, {
      threshold: 0.5,
      rootMargin: '0px'
    });

    allSections.forEach(section => {
      sectionObserver.observe(section);
    });

    // Count-up animation
    function animateCounter(element) {
      const target = parseInt(element.dataset.target);
      const duration = 2000;
      const increment = target / (duration / 16);
      let current = 0;

      const timer = setInterval(() => {
        current += increment;
        if (current >= target) {
          element.textContent = target + '+';
          clearInterval(timer);
        } else {
          element.textContent = Math.floor(current) + '+';
        }
      }, 16);
    }

    const statsObserver = new IntersectionObserver((entries) => {
      entries.forEach(entry => {
        if (entry.isIntersecting) {
          const counter = entry.target.querySelector('.stat-number');
          if (counter && counter.textContent === '0') {
            animateCounter(counter);
          }
        }
      });
    }, { threshold: 0.5 });

    document.querySelectorAll('.stat-card').forEach(el => statsObserver.observe(el));

    // Auto-rotating carousel - 4 SECONDS
    let currentSlide = 0;
    const slides = document.querySelectorAll('.initiative-card');
    const dots = document.querySelectorAll('.carousel-dot');
    const totalSlides = slides.length;
    let autoRotate;

    function updateCarousel() {
      slides.forEach((slide, index) => {
        slide.classList.remove('active', 'prev', 'next');
        
        if (index === currentSlide) {
          slide.classList.add('active');
        } else if (index === (currentSlide - 1 + totalSlides) % totalSlides) {
          slide.classList.add('prev');
        } else if (index === (currentSlide + 1) % totalSlides) {
          slide.classList.add('next');
        }
      });

      dots.forEach((dot, index) => {
        dot.classList.toggle('active', index === currentSlide);
      });
    }

    function nextSlide() {
      currentSlide = (currentSlide + 1) % totalSlides;
      updateCarousel();
    }

    window.goToSlide = function(index) {
      currentSlide = index;
      updateCarousel();
      resetAutoRotate();
    };

    function startAutoRotate() {
      autoRotate = setInterval(nextSlide, 7000); // Slowed down to 7 seconds
    }

    function resetAutoRotate() {
      clearInterval(autoRotate);
      startAutoRotate();
    }

    updateCarousel();
    startAutoRotate();

    const carouselContainer = document.querySelector('.carousel-container');
    if (carouselContainer) {
      carouselContainer.addEventListener('mouseenter', () => {
        clearInterval(autoRotate);
      });
      
      carouselContainer.addEventListener('mouseleave', () => {
        startAutoRotate();
      });
    }
    
    // MathJax config
    window.MathJax = {
      tex: {
        inlineMath: [['\\(', '\\)']],
        displayMath: [['\\[', '\\]']]
      }
    };
  </script>
</Layout>
